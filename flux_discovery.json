{
  "discovery": {
    "wind_stress": {
      "question": "Stations with Wind Stress",
      "search": [
          {
            "join": "or",
            "short_names": ["TAUX","TAUY"],
            "standard_names": ["surface_downward_eastward_stress","surface_downward_northward_stress"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_keo_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_papa_flux"
            ]
          },
          {
            "join": "or",
            "short_names": ["TAUE","TAUN"],
            "standard_name": ["surface_downward_eastward_stress","surface_downward_northward_stress"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/WHOI_flux"
            ]
          }
      ]
    },
    "net_surface_flux": {
      "question": "Stations with Net Surface Heat Flux",
      "search":[
          {
            "join": "or",
            "short_names": ["QNET"],
            "standard_names": ["surface_net_heat_flux"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_keo_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_papa_flux"
            ]
          },
          {
            "join": "or",
            "short_names": ["QN"],
            "standard_names": ["surface_net_heat_flux"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/WHOI_flux"
            ]
          }
      ]
    },
    "turbulent_heat_flux": {
      "question": "Stations with Bulk Turbulent Heat Flux",
      "search": [
          {
            "join": "and",
            "short_names": ["QLAT", "QSEN"],
            "standard_names":["surface_upward_latent_heat_flux", "surface_upward_sensible_heat_flux"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_keo_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_papa_flux"
            ]
          },
          {
            "join": "and",
            "short_names": ["QL", "QS"],
            "standard_names":["surface_upward_latent_heat_flux", "surface_upward_sensible_heat_flux"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/WHOI_flux"
            ]
          }
        ]
    },
    "precip": {
      "question": "Stations with Precipitation",
      "search": [
          {
            "join": "and",
            "short_names": ["RAIN"],
            "standard_names": ["rainfall_rate"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_keo_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_papa_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/WHOI_flux"
            ]
          }
        ]
    },
    "evap_minus_precip": {
      "question": "Stations with Evaporation minus Precipitation",
      "search": [
          {
            "join": "and",
            "short_names": ["RAIN", "EVAP"],
            "standard_names": ["rainfall_rate", "surface_water_evaporation_rate"],
            "datasets": [
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_relative",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/tao_flux_clim_lwr_absolute",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_keo_flux",
              "http://hazy.pmel.noaa.gov:8140/erddap/tabledap/ocs_papa_flux"
            ]
          }
        ]
    }
  }
}
